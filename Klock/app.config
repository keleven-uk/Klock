<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="Klock.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <userSettings>
        <Klock.My.MySettings>
            <setting name="usrFormColour" serializeAs="String">
                <value>Info</value>
            </setting>
            <setting name="usrFormFont" serializeAs="String">
                <value>Tahoma, 9.75pt</value>
            </setting>
            <setting name="usrFormFontColour" serializeAs="String">
                <value>Black</value>
            </setting>
            <setting name="usrFormTop" serializeAs="String">
                <value>100</value>
            </setting>
            <setting name="usrFormLeft" serializeAs="String">
                <value>100</value>
            </setting>
            <setting name="usrSavePos" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="usrTimerHigh" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="usrTimerClearSplit" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="usrTimeSwatchCentibeats" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="usrTimeNETSeconds" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="usrTimeDisplayMinimised" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="usrTimeHourPips" serializeAs="String">
                <value>True</value>
            </setting>
        </Klock.My.MySettings>
    </userSettings>
</configuration>
